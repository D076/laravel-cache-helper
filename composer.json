{
    "name": "d076/laravel-cache-helper",
    "description": "Small helper for Laravel Cache facade",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Titov",
            "email": "dezmond076@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/laravel": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "D076\\LaravelCacheHelper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "D076\\LaravelCacheHelper\\CacheHelperServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
